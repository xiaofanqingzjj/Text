<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.gplibs.magic_surface_view_sample">

    <!-- talking data -->
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
    <uses-permission android:name="android.permission.GET_TASKS"/>

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.Magic">
        <activity android:name=".launch.LaunchActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity">
        </activity>
        <activity android:name=".MacWindowAnimActivity">
        </activity>
        <activity android:name=".WaveAnimActivity">
        </activity>
        <activity android:name=".VortexAnimActivity">
        </activity>
        <activity android:name=".MatrixAnimSampleActivity">
        </activity>
        <activity android:name=".LightSampleActivity">
        </activity>
        <activity android:name=".MultiScrapAnimActivity">
        </activity>
        <activity android:name=".MultiSlideAnimActivity">
        </activity>
    </application>

</manifest>